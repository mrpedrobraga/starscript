{
    "cSpell.enableFiletypes": [
        "starscript"
    ],
    "cSpell.language": "en-GB",
    "inlineFold.supportedLanguages": [
        "astro",
        "vue",
        "html",
        "svelte",
        "vue-html",
        "php",
        "blade",
        "erb",
        "twig",
        "nunjucks",
        "django-html",
        "jinja-html",
        "javascript",
        "typescript",
        "javascriptreact",
        "typescriptreact",
        "starscript"
    ],
    "inlineFold.regex": "(data:image/\\w+;base64(.*))",
    "inlineFold.regexGroup": "1",
    "inlineFold.maskChar": "[base64 image]",
    "inlineFold.regexFlags": "mg"
}