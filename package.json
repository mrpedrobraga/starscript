{
  "name": "starscript",
  "displayName": "StarScript",
  "description": "StarScript for Visual Studio Code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "starscript",
      "aliases": ["StarScript", "starscript"],
      "extensions": [".ssh",".star",".starscript",".ssobj"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "starscript",
      "scopeName": "source.starscript",
      "path": "./syntaxes/starscript.tmLanguage.json",
      "embeddedLanguages": {
        "source.js": "javascript",
        "meta.embedded.block.javascript": "javascript",
        "source.gdscript": "gdscript",
        "meta.embedded.block.gdscript": "gdscript",
        "source.glsl": "glsl",
        "meta.embedded.block.glsl": "glsl",
        "source.shellscript": "sh",
        "meta.embedded.block.shellscript": "sh",
        "source.batch": "batch",
        "meta.embedded.block.batch": "batch"
      }
    }]
  }
}
